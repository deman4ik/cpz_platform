{
  "name": "cpz_platform",
  "version": "1.0.0",
  "description": "Cryptuoso Cryptocurrency Trading Platform",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --env --config moleculer.config.ts services/**/*.service.ts",
    "api": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --env --config api.config.ts services/api.service.ts",
    "importer": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --env --config importer.config.ts services/importer/importer-runner.service.ts services/importer/importer-worker.service.ts",
    "importerWorker": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --env --instances=2 --config importerWorker.config.ts services/importer/importer-worker.service.ts services/connector/public-connector.service.ts",
    "db": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --env --config db.config.ts services/db/candles/*.service.ts services/db/*.service.ts",
    "connector": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --env --config connector.config.ts services/connector/*.service.ts",
    "exwatcher": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --env --config exwatcher.config.ts services/exwatcher.service.ts services/connector/public-connector.service.ts",
    "backtesterWorker": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --env --config importerWorker.config.ts services/backtester/backtester-worker.service.ts services/db/candles/*.service.ts services/db/backtest/*.service.ts",
    "cli": "moleculer connect",
    "start": "moleculer-runner dist/services",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "tslint -p tsconfig.json",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "cryptuoso",
    "cryptucurrency",
    "trading",
    "bitcoin"
  ],
  "author": "Dmitry Shatalov (@deman4ik)",
  "devDependencies": {
    "@types/async-retry": "^1.4.1",
    "@types/bluebird": "^3.5.24",
    "@types/bull": "^3.10.2",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/node-cron": "^2.0.2",
    "@types/node-fetch": "^2.5.0",
    "@types/require-from-string": "^1.2.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^10.11.2",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "mockdate": "^2.0.5",
    "moleculer-repl": "^0.5.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb-base": "^0.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "async-retry": "^1.2.3",
    "ccxt": "^1.18.1045",
    "dayjs": "^1.8.15",
    "dotenv": "^8.0.0",
    "fastest-validator": "^0.6.17",
    "https-proxy-agent": "^2.2.2",
    "ioredis": "^4.14.0",
    "microjob": "^0.6.1",
    "moleculer": "^0.14.0-beta3",
    "moleculer-bull": "^0.2.4",
    "moleculer-db": "^0.8.2",
    "moleculer-db-adapter-sequelize": "^0.2.2",
    "moleculer-web": "^0.8.0",
    "nats": "^1.0.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "redlock": "^4.0.0",
    "require-from-string": "^2.0.2",
    "sequelize": "^5.14.0",
    "socket.io-client": "^2.2.0",
    "tulind": "^0.8.18",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  }
}
